{
  "name": "@techminers/one-web",
  "version": "0.0.1",
  "description": "TechMiners ONE - A powerful app to assess technology powered by deep insights from automated data analysis.",
  "author": "TechMiners <dd@techminers.org>",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "coverage": "npm test -- --coverage --watchAll=false",
    "generate-fetcher-poll": "restful-react import --url http://127.0.0.1:8000/forms/api/swagger.json --output src/api-client/poll-api.tsx",
    "postgenerate-fetcher-poll": "prettier src/api-client/poll-api.tsx --write",
    "generate-fetcher-one": "restful-react import --url http://127.0.0.1:8001/one/api/swagger.json --output src/api-client/one-api.tsx",
    "postgenerate-fetcher-one": "prettier src/api-client/one-api.tsx --write",
    "generate-fetcher-report-generator": "restful-react import --url http://127.0.0.1:8002/openapi.json --output src/api-client/report-generator-api.tsx",
    "postgenerate-fetcher-report-generator": "prettier src/api-client/report-generator-api.tsx --write"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ],
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.19.2",
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.11.3",
    "@material-ui/system": "^4.12.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.34",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "css-vendor": "^2.0.8",
    "date-fns": "^2.28.0",
    "echarts": "^4.9.0",
    "echarts-for-react": "^2.0.16",
    "jss": "^10.8.2",
    "jwt-decode": "^3.1.2",
    "material-ui-dropzone": "^3.4.0",
    "material-ui-image": "^3.2.3",
    "notistack": "^1.0.10",
    "path": "^0.12.7",
    "polished": "^4.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.4",
    "react-helmet": "^6.1.0",
    "react-markdown": "^5.0.3",
    "react-perfect-scrollbar": "^1.5.8",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.1",
    "restful-react": "^15.9.2",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.3",
    "http-proxy-middleware": "^1.0.5",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "nock": "^13.2.1",
    "prettier": "^2.4.1"
  }
}
